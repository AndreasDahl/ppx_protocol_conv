(jbuild_version 1)

(library
 ((name deriving_protocol_json)
  (public_name ppx_deriving_protocol.json)
  (modules (Json))
  (libraries (deriving_protocol base yojson))
  (synopsis "json (de)serialization driver for ppx_deriving_protocol")
 )
)

(library
 ((name deriving_protocol_xml)
  (public_name ppx_deriving_protocol.xml)
  (modules (Xml_light))
  (libraries (deriving_protocol base xml-light))
  (synopsis "xml-light (de)serialization driver for ppx_deriving_protocol")
 )
)

(library
 ((name deriving_protocol_msgpack)
  (public_name ppx_deriving_protocol.msgpack)
  (modules (msgpack))
  (libraries (deriving_protocol base msgpck))
  (synopsis "MessagePack (de)serialization driver for ppx_deriving_protocol")
 )
)
